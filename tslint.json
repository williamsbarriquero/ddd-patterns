{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "plugins": [
    "prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "ban-ts-ignore": true
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "node_modules"
    ]
  }
}